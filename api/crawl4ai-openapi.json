                ],
                "title": "CrawlJobPayload"
            },
            "CrawlRequestWithHooks": {
                "properties": {
                    "urls": {
                        "items": {
                            "type": "string"
                        },
                        "type": "array",
                        "maxItems": 100,
                        "minItems": 1,
                        "title": "Urls"
                    },
                    "browser_config": {
                        "anyOf": [
                            {
                                "additionalProperties": true,
                                "type": "object"
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "title": "Browser Config"
                    },
                    "crawler_config": {
                        "anyOf": [
                            {
                                "additionalProperties": true,
                                "type": "object"
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "title": "Crawler Config"
                    },
                    "hooks": {
                        "anyOf": [
                            {
